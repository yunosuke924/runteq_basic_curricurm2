<%= form_with(model: board, local: true) do |form| %> 
  <div class="form-group">
    <%= form.label :title %><br/>
    <%= form.text_field :title, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :body %><br/>
    <%= form.text_area :body, cols: "20", rows: "10", class: "form-control" %>
  </div>
  <div class="actions">
    <%= form.submit class: "btn btn-primary" %>
  </div>
<% end %>